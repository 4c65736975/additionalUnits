<?xml version="1.0" encoding="utf-8" standalone="no"?>
<GUIProfiles>
  <Presets>
    <Preset name="colorTransparent" value="0 0 0 0"/>
  </Presets>

  <Traits>
    <Trait name="anchorTopLeft">
      <Value name="anchors" value="0 0 1 1"/>
    </Trait>
    <Trait name="anchorMiddleLeft">
      <Value name="anchors" value="0 0 0.5 0.5"/>
    </Trait>
    <Trait name="anchorMiddleCenter">
      <Value name="anchors" value="0.5 0.5 0.5 0.5"/>
    </Trait>
    <Trait name="anchorMiddleRight">
      <Value name="anchors" value="1 1 0.5 0.5"/>
    </Trait>
  </Traits>

